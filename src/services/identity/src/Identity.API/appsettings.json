{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoggerOptions": {
    "Level": "Information",
    "LogTemplate": "{Timestamp:HH:mm:ss} [{Level:u4}] {Message:lj}{NewLine}{Exception}",
    "StructuredLoggerOptions": {
      "Enable": true
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5433;Database=identityDb;Username=postgres;Password=admin;Include Error Detail=true"
  },
  "Jwt": {
    "Authority": "https://localhost:5200",
    "Audience": "identity-api"
  }
}
