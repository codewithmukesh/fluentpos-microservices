{
  "AppConfig": {
    "Name": "Identity API"
  },
  "AllowedHosts": "*",
  "LoggerConfig": {
    "Level": "Information",
    "LogTemplate": "{Timestamp:HH:mm:ss} [{Level:u4}] {Message:lj}{NewLine}{Exception}",
    "StructuredLoggerOptions": {
      "Enable": true
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5433;Database=identityDb;Username=admin;Password=admin;Include Error Detail=true"
  },
  "AuthConfig": {
    "Authority": "https://localhost:5200",
    "Audience": "identity-api"
  }
}
