{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoggerOptions": {
    "Level": "Debug",
    "LogTemplate": "{Timestamp:HH:mm:ss} [{Level:u4}] {Message:lj}{NewLine}{Exception}",
    "StructuredLoggerOptions": {
      "Enable": true
    }
  },
  "Jwt": {
    "Authority": "https://localhost:6100"
  },
  "ReverseProxy": {
    "routes": {
      "catalog": {
        "clusterId": "catalog",
        "match": {
          "path": "/catalog/{**catch-all}"
        },
        "transforms": [
          {
            "pathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "clusters": {
      "catalog": {
        "destinations": {
          "catalog": {
            "address": "http://localhost:5020"
          }
        }
      }
    }
  }
}
